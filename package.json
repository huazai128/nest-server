    "geoip-lite": "^1.4.10",
    "google-protobuf": "^3.21.2",
    "ioredis": "^5.4.1",
    "js-base64": "^3.7.7",
    "mongoose": "^8.4.1",
    "mongoose-aggregate-paginate-v2": "^1.1.2",
    "mongoose-paginate-v2": "^1.8.2",
    "object-sizeof": "^2.6.5",
    "passport": "^0.7.0",
    "puppeteer": "^23.1.0",
    "reflect-metadata": "^0.2.0",
    "request": "^2.88.2",
    "rxjs": "^7.8.1",
    "source-map": "^0.7.4",
    "ua-parser-js": "^2.0.0"
